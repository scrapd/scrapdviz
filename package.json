{
  "name": "scrapdviz",
  "version": "0.4.0",
  "description": "Visualize Austin traffic fatalities from another angle",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "ci": "run-s lint:* test",
    "dev": "node server.js",
    "dump": "NODE_ENV=production run-s build export",
    "export": "next export",
    "fix": "run-s fix:*",
    "fix:format": "prettier --write \"pages/**/*.js\" \"components/**/*.js\"",
    "fix:javascript": "eslint --ext js,html --fix pages components csp.js server.js",
    "lint": "run-s lint:*",
    "lint:javascript": "eslint --ext js,html pages components csp.js server.js",
    "ship": "run-s dump && ./tools/deploy.sh",
    "start": "NODE_ENV=production node server.js",
    "test": "jest",
    "test:update": "jest -u",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "litereport": "lighthouse http://localhost:8000/ --output json --output csv --output html --output-path litereports/Lite.html --view | lighthouse-ci http://localhost:8000/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgreinho/scrapdviz.git"
  },
  "keywords": [
    "dashboard",
    "statistics",
    "austin"
  ],
  "author": "RÃ©my Greinhofer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rgreinho/scrapdviz/issues"
  },
  "homepage": "https://github.com/rgreinho/scrapdviz#readme",
  "dependencies": {
    "@emotion/core": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.10",
    "@zeit/next-css": "^1.0.1",
    "antd": "^3.26.17",
    "babel-plugin-import": "^1.13.0",
    "express": "^4.17.1",
    "facepaint": "^1.2.1",
    "helmet": "^3.22.1",
    "lighthouse": "^8.1.0",
    "lighthouse-ci": "^1.13.0",
    "lru-cache": "^5.1.1",
    "mapbox-gl": "^1.11.0",
    "moment": "^2.26.0",
    "next": "^11.1.0",
    "next-redux-wrapper": "^4.0.1",
    "react": "^16.12.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.12.0",
    "react-mapbox-gl": "^4.8.6",
    "react-redux": "^7.1.3",
    "redux-saga": "^1.1.3",
    "snyk": "^1.667.0",
    "victory": "^34.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-dynamic-import-node": "^2.3.3",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-purify-webpack-loader": "^1.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-standard": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.2",
    "next-workbox-webpack-plugin": "^2.1.1",
    "npm-run-all": "^4.1.5",
    "null-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "prettier-eslint": "^9.0.2",
    "react-test-renderer": "^16.13.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "webpack-pwa-manifest": "^4.2.0"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "babel-plugin-dynamic-import-node",
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ],
      [
        "emotion"
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "parser": "babel-eslint",
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "16.0"
      },
      "propWrapperFunctions": [
        "forbidExtraProps"
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": "off",
      "react/react-in-jsx-scope": "off",
      "react/display-name": "off"
    },
    "parserOptions": {
      "ecmaVersion": 2015,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "enzyme",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "snyk": true
}
