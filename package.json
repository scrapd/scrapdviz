{
  "name": "scrapdviz",
  "version": "0.2.0",
  "description": "Visualize Austin traffic fatalities from another angle",
  "main": "index.js",
  "scripts": {
    "build": "next build",
    "ci": "run-s lint:* test",
    "dev": "node server.js",
    "dump": "run-s build export",
    "export": "next export",
    "fix": "run-s fix:*",
    "fix:format": "prettier --write \"pages/**/*.js\" \"components/**/*.js\"",
    "fix:javascript": "eslint --ext js,html --fix pages components",
    "lint": "run-s lint:*",
    "lint:javascript": "eslint --ext js,html pages components",
    "ship": "run-s dump && ./tools/deploy.sh",
    "start": "NODE_ENV=production node server.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rgreinho/scrapdviz.git"
  },
  "keywords": [
    "dashboard",
    "statistics",
    "austin"
  ],
  "author": "RÃ©my Greinhofer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rgreinho/scrapdviz/issues"
  },
  "homepage": "https://github.com/rgreinho/scrapdviz#readme",
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@zeit/next-css": "^1.0.1",
    "antd": "^3.13.6",
    "babel-plugin-import": "^1.11.0",
    "express": "^4.16.4",
    "facepaint": "^1.2.1",
    "lru-cache": "^5.1.1",
    "mapbox-gl": "^0.53.1",
    "moment": "^2.24.0",
    "next": "^7.0.3",
    "next-redux-wrapper": "^3.0.0-alpha.2",
    "react": "^16.8.6",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.8.3",
    "react-mapbox-gl": "^4.2.0",
    "react-redux": "^6.0.1",
    "redux-saga": "^1.0.2",
    "victory": "^32.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "css-purify-webpack-loader": "^1.0.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.15.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.1.0",
    "jest-enzyme": "^7.0.1",
    "next-workbox-webpack-plugin": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "react-test-renderer": "^16.8.3",
    "redux-devtools-extension": "^2.13.8",
    "webpack-pwa-manifest": "^4.0.0"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      "babel-plugin-dynamic-import-node",
      [
        "import",
        {
          "libraryName": "antd",
          "style": "css"
        }
      ],
      [
        "emotion"
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "settings": {
      "react": {
        "createClass": "createReactClass",
        "pragma": "React",
        "version": "16.0"
      },
      "propWrapperFunctions": [
        "forbidExtraProps"
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-console": "off",
      "react/react-in-jsx-scope": "off",
      "react/display-name": "off"
    },
    "parserOptions": {
      "ecmaVersion": 2015,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "testEnvironment": "enzyme",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  }
}
